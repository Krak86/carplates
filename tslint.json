{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "indent": [true, "spaces"],
        "max-line-length": false,
        "jsx-no-multiline-js": false,
        "jsx-wrap-multiline": false,
        "jsx-no-lambda": false
    },
    "rulesDirectory": []
}